{
  "name": "test-o-tron",
  "version": "1.0.0",
  "description": "use lichess analysis from past games",
  "main": "dist/main.js",
  "author": "tailuge",
  "license": "GPL-3.0",
  "scripts": {
    "test": "nyc mocha --require ts-node/register -r jsdom-global/register ./test/*.spec.ts",
    "watch": "gulp"
  },
  "dependencies": {
    "chess.js": "^0.10",
    "chessground": "^7.3",
    "oboe": "^2.1.4",
    "page": "^1.8.6",
    "snabbdom": "^0.7.1"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.1",
    "browserify": "^16.2.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "gulp": "^3",
    "gulp-mocha": "^6.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^4.0.2",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.1",
    "prettier": "^1.13.4",
    "ts-node": "^6.0.5",
    "tsify": "^4.0.0",
    "typescript": "^2.9.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.0"
  },
  "nyc": {
    "include": [
      "./src/*.ts"
    ],
    "exclude": [
      ".test/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
