{
  "name": "mistake-o-tron",
  "version": "1.0.0",
  "description": "reuse lichess analysis from past games",
  "main": "dist/main.js",
  "author": "tailuge",
  "license": "GPL-3.0",
  "scripts": {
    "test": "nyc mocha --require ts-node/register -r jsdom-global/register ./test/*.spec.ts",
    "deps": "npx yarn-check --skip-unused",
    "watch": "gulp"
  },
  "dependencies": {
    "chess.js": "^0.10.3",
    "chessground": "~7.3",
    "lodash": "^4.17.21",
    "lodash.template": "^4.5.0",
    "mem": "^6.0.1",
    "oboe": "^2.1.5",
    "page": "^1.11.5",
    "snabbdom": "^0.7.4"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.1",
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "gulp": "^4.0.2",
    "gulp-mocha": "^7.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-terser": "^1.2.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3",
    "jsdom": "16.2.0",
    "jsdom-global": "3.0.2",
    "mocha": "^7.1.0",
    "nyc": "^12.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "tsify": "^4.0.1",
    "typescript": "^3.8.3",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1"
  },
  "nyc": {
    "include": [
      "./src/*.ts"
    ],
    "exclude": [
      ".test/*.spec.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
